<!DOCTYPE html>
<html>
<head>
  <title>TaptaroDataManager</title>
  <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<%= render 'common/navigation' %>

<div class="container">

  <%= render 'common/flash' %>

  <%= yield %>

  <%= render 'common/export' unless current_admin.nil? %>

</div>

</body>
</html>
